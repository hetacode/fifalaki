    "version": "0.2.0",
    "configurations": [
        {
            "name": "Gateway Srv",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Gateway/bin/Debug/net5.0/Gateway.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Gateway",
            "console": "internalConsole",
            "stopAtEntry": false,
        },
        {
            "name": "Game Srv",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Game/bin/Debug/net5.0/Game.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Game",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Games List Srv",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/GamesList/bin/Debug/net5.0/GamesList.dll",
            "args": [],
            "cwd": "${workspaceFolder}/GamesList",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/Rtm",
            "runtimeExecutable": "npm run start",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}